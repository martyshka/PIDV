{
    "name": "cyrkulewski/personal-id-validator",
    "type": "library",
    "description": "Library to validate personal IDs of different countries.",
    "keywords": [
        "Aleksander Cyrkulewski",
        "Personal ID validator"
    ],
    "homepage": "https://github.com/martyshka/pidv",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksander Cyrkulewski",
            "email": "cyrkulewski@gmail.com",
            "homepage": "http://cyrkulewski.com"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "cyrkulewski\\PIdV\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cyrkulewski\\PIdV\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
